{
  "name": "voluntry-backend",
  "version": "1.0.0",
  "description": "Volunteer management backend with auth, chat, and stats",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "echo \"No linter configured\"",
    "setup-db": "node run-schema.js",
    "migrate": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:status": "npx knex migrate:status",
    "migrate:make": "npx knex migrate:make"
  },
  "keywords": [
    "volunteer",
    "express",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.11.5",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
